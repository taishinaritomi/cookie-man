{
  "name": "cookie-man",
  "version": "0.0.6",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite --watch build",
    "build": "vite build && zip -r dist.zip dist/",
    "fix": "run-s fix:*",
    "fix:lint": "eslint . --fix --ignore-path .gitignore",
    "fix:format": "prettier . --write --ignore-path .gitignore",
    "check": "run-p check:*",
    "check:type": "tsc --noEmit",
    "check:lint": "eslint . --ignore-path .gitignore",
    "check:format": "prettier . --check --ignore-path .gitignore"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta",
    "@tsconfig/strictest": "^2.0.2",
    "@types/chrome": "^0.0.251",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "solid-headless": "^0.13.1",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.5",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.1.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-solid": "^2.7.2",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
